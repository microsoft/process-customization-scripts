    <DataSet Name="dsIteration">
      <Fields>
        <Field Name="IterationName">
          <DataField>IterationName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StartDate">
          <DataField>StartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="IterationPath">
          <DataField>IterationPath</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FinishDate">
          <DataField>FinishDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Depth">
          <DataField>Depth</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ParameterValue">
          <DataField>ParameterValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Order">
          <DataField>Order</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>TfsReportDS</DataSourceName>
        <CommandText>DECLARE @Results TABLE ( 
	IterationName NVARCHAR(MAX),
	IterationPath NVARCHAR(4000),
	StartDate DATE,
	FinishDate DATE,
	IterationGUID NVARCHAR(256)
)

INSERT @Results
	SELECT	SUBSTRING(IterationPath, CHARINDEX('\', IterationPath, 2) + 1, LEN(IterationPath)) AS IterationName,
	        IterationPath,
			StartDate, 
			FinishDate,
			IterationGUID
	 FROM DimIteration
	WHERE StartDate IS NOT NULL
		AND FinishDate IS NOT NULL
		-- We filter out the root iteration since at minimum this would be the backlog
		-- and not a iteration that you'd measure velocity for
		AND Depth &gt; 0
		AND ProjectGUID = (@ProjectGuid)
	ORDER BY StartDate

-- Return the results with the correct formatting
--
IF @@ROWCOUNT &gt; 0 BEGIN
	SELECT
		wi.IterationName,
		wi.IterationPath, 
		wi.StartDate,
		wi.FinishDate, 
		i.Depth,
		dbo.GetWorkItemIterationKey(Depth, wi.IterationGUID) AS ParameterValue,
		-- Result ordering is by Start, Finish, Path
		-- This is used to order the data in the chart by date
		ROW_NUMBER() OVER(ORDER BY wi.StartDate, wi.FinishDate, wi.IterationPath) AS [Order]
	FROM @Results wi
		INNER JOIN DimIteration i ON i.IterationGUID = wi.IterationGUID
	-- Order by Iteration Path so we get 'by path' ordering in the Iteration Param drop down.
	-- The data series is ordered by the [Order] parameter so we effectively get "by date" ordering
	ORDER BY IterationPath

END ELSE BEGIN
	SELECT 
		@AllNoFilter AS IterationName, 
		@AllNoFilter AS IterationPath, 
		NULL AS StartDate, 
		NULL AS FinishDate, 
		0 AS Depth, 
		'[Work Item].[Iteration Hierarchy].[All]' AS ParameterValue, 
		0 AS [Order]
END</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProjectGuid">
            <Value>=Parameters!ProjectGuid.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllNoFilter">
            <Value>=Parameters!AllNoFilter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
