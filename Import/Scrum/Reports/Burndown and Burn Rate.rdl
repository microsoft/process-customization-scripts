    <DataSet Name="dsAreaSQL">
      <Fields>
        <Field Name="AreaSK">
          <DataField>AreaSK</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AreaName">
          <DataField>AreaName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
