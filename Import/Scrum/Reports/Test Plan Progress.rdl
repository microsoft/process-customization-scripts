      <Query>
        <DataSourceName>TfsOlapReportDS</DataSourceName>
        <CommandText>WITH
MEMBER [Measures].[ParameterCaption] AS
	IIF([Test Plan].[Test Plan State ID].CurrentMember IS [Test Plan].[Test Plan State ID].[All],
		@AllNoFilter,
		[Test Plan].[Test Plan State].CurrentMember.Member_Caption
	)

MEMBER [Measures].[ParameterValue] AS
	[Test Plan].[Test Plan State ID].CurrentMember.UniqueName

SELECT
{
	[Measures].[Point Count Trend],
	[Measures].[ParameterValue],
	[Measures].[ParameterCaption]
} ON COLUMNS,
{
	([Test Plan].[Test Plan State ID].[All], [Test Plan].[Test Plan State].[All]) +
	Extract(
		(
			[Test Plan].[Test Plan State ID].[All], 
			[Test Plan].[Test Plan State].[All],
			[Team Project].[Project Node GUID].[All],
			[Test Case].[Area Hierarchy].[All],
			[Test Plan].[Iteration Hierarchy].[All],
			[Test Plan].[Test Plan ID].[All]
		) +
		Filter(
			CrossJoin(
				[Test Plan].[Test Plan State ID].Children,
				Exists([Test Plan].[Test Plan State].[Test Plan State], [Test Plan].[Test Plan State ID].CurrentMember),
				StrToMember("[Team Project].[Project Node GUID].&amp;[{" + @ProjectGuid + "}]"),
				StrToSet(@AreaParam),
				StrToSet(@IterationParam),
				StrToSet(@TestPlanParam)
				),
			[Measures].[Point Count Trend]
		),
		[Test Plan].[Test Plan State ID], [Test Plan].[Test Plan State]
	)
} ON ROWS
FROM [Team System]</CommandText>
