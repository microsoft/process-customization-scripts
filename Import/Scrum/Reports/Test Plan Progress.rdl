    <DataSet Name="dsAreaSQL">
      <Fields>
        <Field Name="AreaSK">
          <DataField>AreaSK</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AreaName">
          <DataField>AreaName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>TfsReportDS</DataSourceName>
        <CommandText>;WITH AreaKey(AreaSK, AreaName, Depth, AreaKey) AS
(
	SELECT AreaSK, AreaName, Depth,
		'.&amp;[' +
			CONVERT(nvarchar, CONVERT(bigint, SUBSTRING(CONVERT(binary(16), CONVERT(uniqueidentifier, AreaGUID)), 1, 8))) +
			']&amp;[' +
			CONVERT(nvarchar, CONVERT(bigint, SUBSTRING(CONVERT(binary(16), CONVERT(uniqueidentifier, AreaGUID)), 9, 8))) +
		']'
	  FROM DimArea
)

SELECT '*' AS AreaSK, 'All' AS AreaName
WHERE '[Test Case].[Area Hierarchy].[All]' IN (@AreaParam)

UNION

SELECT DISTINCT 
	CASE WHEN Depth = 0 THEN '*' ELSE CAST(AreaSK AS NVARCHAR(255)) END AS AreaSK, 
	CASE WHEN Depth = 0 THEN 'All' ELSE AreaName END
  FROM AreaKey
 WHERE
	'[Test Case].[Area Hierarchy].[Area' + CONVERT(varchar, Depth) + ']' + AreaKey IN (@AreaParam)
    OR	
	'[Test Case].[Area Hierarchy].[Area' + CONVERT(varchar, Depth + 1) + ']' + AreaKey IN (@AreaParam)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@AreaParam">
            <Value>=Parameters!AreaParam.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
