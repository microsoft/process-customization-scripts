        </Field>
        <Field Name="ParameterLevel">
          <DataField>&lt;?xml version="1.0" encoding="utf-8"?&gt;&lt;Field xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:type="Measure" UniqueName="[Measures].[ParameterLevel]" /&gt;</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>TfsOlapReportDS</DataSourceName>
        <CommandText>WITH
	MEMBER [Measures].[Add Spaces] AS
		VBA.Space(([Test Case].[Area Hierarchy].CurrentMember.Level.Ordinal - 1) * 2)
	MEMBER [Measures].[ParameterCaption] AS
		IIF([Test Case].[Area Hierarchy].CurrentMember IS [Test Case].[Area Hierarchy].[All],
			@AllNoFilter,
			[Measures].[Add Spaces] + [Test Case].[Area Hierarchy].CurrentMember.Member_Caption
		)
	MEMBER [Measures].[ParameterValue] AS [Test Case].[Area Hierarchy].CurrentMember.UniqueName
	MEMBER [Measures].[ParameterLevel] AS [Test Case].[Area Hierarchy].CurrentMember.Level.Ordinal
	MEMBER [Measures].[EmptyOrZeroPointCountTrend] AS 
	IIF (ISEMPTY([Measures].[Point Count Trend]) OR [Measures].[Point Count Trend] = 0,
		null,
		1
	)
SELECT
{
	[Measures].[ParameterCaption],
	[Measures].[ParameterValue],
	[Measures].[ParameterLevel]
} ON COLUMNS,
{
	Union(
		[Test Case].[Area Hierarchy].[All],
		Descendants(
			//
			// We use Except to remove the extra [All] member so we get the descendants only of the
			// team project.
			//
			Except(
				Extract(
					Union(
						//
						// We add [All] so Extract always has at least one tuple to work with. Otherwise
						// it produces an error when there are no tuples returned by the NonEmpty below.
						//
						CrossJoin(
							StrToMember("[Team Project].[Project Node GUID].[All]"),
							[Test Case].[Area Hierarchy].[All]
						),
						NonEmpty(
							CrossJoin(
								StrToMember("[Team Project].[Project Node GUID].&amp;[{" + @ProjectGuid + "}]"),
								[Test Case].[Area Hierarchy].[Area0]
							),
							[Measures].[EmptyOrZeroPointCountTrend]
						)
					),
					[Test Case].[Area Hierarchy]
				),
				[Test Case].[Area Hierarchy].[All]
			)
		)
	)
} ON ROWS
FROM [Team System]</CommandText>
        <QueryParameters>
          <QueryParameter Name="ProjectGuid">
            <Value>=Parameters!ProjectGuid.Value</Value>
          </QueryParameter>
          <QueryParameter Name="AllNoFilter">
            <Value>=Parameters!AllNoFilter.Value</Value>
          </QueryParameter>
        </QueryParameters>
