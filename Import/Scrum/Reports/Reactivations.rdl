WITH
//
// Define a named set that includes all of the previous states, except for the set of states
// given here (the "-" at the start means exclude). Basically, this returns Resolved and
// Closed when you have the three states Active, Resolved, and Closed.
//
SET [None Active Previous States] AS
    Except(    
        Descendants(StrToSet(@PreviousStateParam)),
        {
            StrToSet(@PreviousStatesToExclude),
            [Work Item].[Previous State].[All].UnknownMember,
            [Work Item].[Previous State].[All]
        }
    )

//
// The number of work items that were still reactivated at the end of the day
// (because of the filter at the very bottom that only looks at Active).
//
MEMBER [Measures].[Cumulative Reactivation Count] AS
	Sum(
	    [None Active Previous States],
		[Measures].[Work Item Count]
	) + 0                               // Prevent null values from being returned

//
// The number of times during a day that work items were reactivated. If a work item
// was reactivated and resolved in one day, it will be counted here but not in the
// [Cumulative Reactivation Count].
//
MEMBER [Measures].[Daily Reactivation Count] AS
	CoalesceEmpty(
	    Sum(
			[None Active Previous States],
			[Measures].[State Change Count]
		),
		0
	) + 0                               // Prevent null values from being returned

MEMBER [Measures].[Date Value] AS
    [Date].[Date].CurrentMember.Member_Value

SELECT
{
    [Measures].[Date Value],
	[Measures].[Work Item Count],
	[Measures].[Cumulative Reactivation Count],
	[Measures].[Daily Reactivation Count]
} ON COLUMNS,
(
    Filter(
        [Date].[Date].[Date],
        [Date].[Date].CurrentMember.Member_Value &gt;= CDate(@StartDateParam) AND
        [Date].[Date].CurrentMember.Member_Value &lt;= CDate(@EndDateParam)
    ) *
    [Work Item].[System_State].[System_State]
) ON ROWS
FROM
(
    SELECT
    (
        CrossJoin(
            StrToMember("[Team Project].[Project Node GUID].&amp;[{" + @ProjectGuid + "}]"),
            StrToSet(@AreaParam),
	        StrToSet(@IterationParam),            
            {
                StrToMember("[Work Item].[System_State].&amp;[" + @Active + "]"),
                StrToMember("[Work Item].[System_State].&amp;[" + @Resolved + "]")
            },
            Except(
                Descendants(StrToSet(@WorkItemTypeParam)),
                [Work Item].[System_WorkItemType].[All] + StrToSet(@WorkItemTypesToExclude)
            )
        )
    ) ON COLUMNS
    FROM [Team System]
)</CommandText>
