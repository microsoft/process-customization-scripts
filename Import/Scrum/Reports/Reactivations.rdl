            <Value>=Code.CollectionToMDX(Parameters!PreviousStatesToExclude.Value, "[Work Item].[Previous State].&amp;[", "]")</Value>
          </QueryParameter>
          <QueryParameter Name="WorkItemTypesToExclude">
            <Value>=Code.CollectionToMDX(Parameters!WorkItemTypesToExclude.Value, "[Work Item].[System_WorkItemType].&amp;[", "]")</Value>
          </QueryParameter>
        </QueryParameters>
