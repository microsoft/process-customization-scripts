    <DataSet Name="dsReleasePath">
      <Query>
        <DataSourceName>TfsReportDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProjectGUID">
            <Value>=Parameters!ProjectGuid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
DECLARE @Selected TABLE (IterationGUID NVARCHAR(256));

-- Insert the iteration nodes into our temporary table
WITH ReleasePath (IterationGUID, ParentIterationGUID)
AS
(
    -- Collect all nodes which have dates
    SELECT IterationGUID, ParentIterationGUID
    FROM DimIteration
    WHERE StartDate IS NOT NULL AND FinishDate IS NOT NULL
    AND ProjectGUID = @ProjectGUID
    
    UNION ALL
    
    -- Find all ancestors of the nodes with dates
    SELECT i.IterationGUID, i.ParentIterationGUID
    FROM DimIteration i
        INNER JOIN ReleasePath AS p
            ON p.ParentIterationGUID = i.IterationGUID
    WHERE i.ParentIterationGUID IS NOT NULL
)
INSERT INTO @Selected
SELECT DISTINCT ParentIterationGUID
FROM ReleasePath;


-- Return the results (or the "Empty" data)
IF @@ROWCOUNT &gt; 0
    -- Return the iteration data
    SELECT  
        i.IterationGUID,
        SPACE(i.Depth) + i.IterationName AS IterationName,
        dbo.GetWorkItemIterationKey(i.Depth, i.IterationGUID) AS IterationMember
    FROM DimIteration i
        INNER JOIN @Selected s ON s.IterationGUID = i.IterationGUID
    ORDER BY i.IterationPath;
ELSE
    -- No data found so return the "Empty" data value
    SELECT IterationGUID, IterationName, '[Work Item].[Iteration Hierarchy].[All]' AS IterationMember
    FROM DimIteration
    WHERE ProjectGUID = @ProjectGUID
    AND Depth = 0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="IterationName">
          <DataField>IterationName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IterationGUID">
          <DataField>IterationGUID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IterationMember">
          <DataField>IterationMember</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsIterations">
      <Query>
        <DataSourceName>TfsReportDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ReleaseGUID">
            <Value>=Parameters!Release.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
DECLARE @ReleasePath NVARCHAR(4000);
DECLARE @ReleaseDepth INT;

SELECT @ReleasePath = IterationPath, @ReleaseDepth = Depth
FROM DimIteration
WHERE IterationGUID = @ReleaseGUID;

DECLARE @Iterations TABLE (
    ReleaseAndIterationPath NVARCHAR(4000),
    IterationPath NVARCHAR(256),
    StartDate DATETIME,
    StartDateMDX NVARCHAR(256),
    ReleaseMember NVARCHAR(256),
    StartDateAndIterationTuple NVARCHAR(4000),
    [Order] INT
);

WITH Candidates (IterationGUID)
AS
(
    -- Starting set is the children of the release path node
    SELECT i.IterationGUID
    FROM DimIteration i
    WHERE i.ParentIterationGUID = @ReleaseGUID

    UNION ALL

    -- Find all descendent nodes
    SELECT i.IterationGUID
    FROM DimIteration i
        INNER JOIN Candidates c ON i.ParentIterationGUID = c.IterationGUID
)
INSERT INTO @Iterations
SELECT 
    -- Returned the release path paired with the Iteration path. 
    -- We do this so that re-evaluation of the RDL report parameters is forced
    -- when we change the release path. Otherwise if you select a child release
    -- path, then change the selection to one of its ancestors, not all of the
    -- values in the IterationParam are selected
    @ReleasePath + ':' + i.IterationPath AS ReleaseAndIterationPath,
    i.IterationPath,
    i.StartDate AS StartDate,
    '[Date].[Date].&amp;[' + CONVERT(CHAR(19), i.StartDate, 126) + ']' AS StartDateMDX,
    dbo.GetWorkItemIterationKey(@ReleaseDepth, @ReleaseGUID) AS ReleaseMember,
    '(' + dbo.GetWorkItemIterationKey(@ReleaseDepth, @ReleaseGUID) + ',' +
        '[Work Item].[Iteration Path].&amp;[' + i.IterationPath + '],' +
        '[Date].[Date].&amp;[' + CONVERT(CHAR(19), i.StartDate, 126) + ']' +
    ')' AS StartDateAndIterationTuple,
    ROW_NUMBER() OVER(ORDER BY i.StartDate, i.FinishDate, i.IterationPath) AS [Order]
FROM DimIteration i
    INNER JOIN Candidates c ON i.IterationGUID = c.IterationGUID
-- We only report on iterations with start dates since that is how we get
-- the effort accumulated in the cube.
WHERE i.StartDate IS NOT NULL 
AND i.FinishDate IS NOT NULL
ORDER BY i.IterationPath

IF (@@ROWCOUNT &lt;&gt; 0)
    SELECT * FROM @Iterations
ELSE
    SELECT 
        NULL AS ReleaseAndIterationPath,
        NULL AS IterationPath,
        NULL AS StartDate,
        NULL AS StartDateMDX,
        NULL AS ReleaseMember,
        '([Work Item].[Iteration Hierarchy].[All].UNKNOWNMEMBER,' +
        '[Work Item].[Iteration Path].[All].UNKNOWNMEMBER,' +
        '[Date].[Date].[All].UNKNOWNMEMBER)' AS StartDateAndIterationTuple,
        1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="IterationPath">
          <DataField>IterationPath</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ReleaseAndIterationPath">
          <DataField>ReleaseAndIterationPath</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StartDate">
          <DataField>StartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="StartDateMDX">
          <DataField>StartDateMDX</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ReleaseMember">
          <DataField>ReleaseMember</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StartDateAndIterationTuple">
          <DataField>StartDateAndIterationTuple</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Order">
          <DataField>Order</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsReleaseMember">
      <Query>
        <DataSourceName>TfsReportDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Release">
            <Value>=Parameters!Release.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT '[Work Item].[Iteration Hierarchy].[Iteration' + CONVERT(varchar, Depth) + '].&amp;[' +
    CONVERT(nvarchar, CONVERT(bigint, SUBSTRING(CONVERT(binary(16), CONVERT(uniqueidentifier, IterationGUID)), 1, 8))) +
    ']&amp;[' +
    CONVERT(nvarchar, CONVERT(bigint, SUBSTRING(CONVERT(binary(16), CONVERT(uniqueidentifier, IterationGUID)), 9, 8))) +
    ']' AS ReleaseMember,
    IterationPath
FROM DimIteration
WHERE IterationGUID = @Release ORDER BY IterationPath;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
