  <Code>Function ParametersToString(ByVal values As Object()) As String
        Dim str As System.Text.StringBuilder = New System.Text.StringBuilder()

        For i As Integer = 0 To values.Length - 1
            If i &gt; 0 Then str.Append(", ")
            str.Append(Trim(values(i).ToString))
        Next
        Return str.ToString
    End Function

    Function CollectionToMDX(ByVal values As Object(), ByVal prefix As String, ByVal suffix As String) As String()
        Dim results(0 To values.Length - 1) As String
        For i As Integer = 0 To values.Length - 1
            results(i) = prefix &amp; Trim(values(i).ToString) &amp; suffix
        Next
        Return results
    End Function

    ' This method finds the sort order of an Iteration returned from the chart query in the IterationOrder parameter.
    ' It looks for a match in the parameter label (which contains iteration paths), and then returns the value, which is the order number.
    Public Function GetIterationOrder(ByRef parameterValues As Object(), ByRef parameterLabels As Object(), ByVal IterationPath As String) As Integer
        For i As Integer = 0 To parameterValues.Length - 1
            If parameterLabels(i) = IterationPath Then
                Return parameterValues(i)
            End If
        Next
        Return 99999
    End Function</Code>
