			FROM     [Test Point View] point			
			WHERE [Area_Test.Area Path] IN (SELECT [Area Path] FROM @AreasAndChildren)
			GROUP BY [Test Plan.Name],[Test Pass.Name],[Test Point Dimension.Test Point Key], [Iteration_Test.Iteration Path]
			HAVING SUM([Point Record Count]) &gt; 0
          ) currentPoint
JOIN     [Test Point Dimension] tpd
ON       currentPoint.PointKey = tpd.[Test Point Key] 
GROUP BY currentPoint.[Iteration Path]
