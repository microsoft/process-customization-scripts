      <Query>
        <DataSourceName>TfsReportDS</DataSourceName>
        <CommandText>DECLARE @OwnValuesTag nvarchar(40);
SET @OwnValuesTag = '[' + @OwnValues + ']';

-- TeamProjectCollection - Needed because links are done at the TPC level
DECLARE @TeamProjectCollectionSK int; 
SET @TeamProjectCollectionSK = (SELECT ParentNodeSK FROM DimTeamProject WHERE ProjectNodeGUID = @ProjectGuid );

-- Link Types
DECLARE @ParentWorkItemLinkTypeSK int; 
SET @ParentWorkItemLinkTypeSK = (SELECT [WorkItemLinkTypeSK] FROM [DimWorkItemLinkType] WHERE [LinkID] &lt; 0 AND [ReferenceName] = 'System.LinkTypes.Hierarchy' AND TeamProjectCollectionSK = @TeamProjectCollectionSK);
DECLARE @ChildWorkItemLinkTypeSK int; 
SET @ChildWorkItemLinkTypeSK = (SELECT [WorkItemLinkTypeSK] FROM [DimWorkItemLinkType] WHERE [LinkID] &gt; 0 AND [ReferenceName] = 'System.LinkTypes.Hierarchy' AND TeamProjectCollectionSK = @TeamProjectCollectionSK);

DECLARE @SelectedAreaList TABLE (AreaSK int, [AreaPath] nvarchar(4000) null);
INSERT @SelectedAreaList 
	SELECT AreaSK, [AreaPath]
	  FROM DimArea
	 WHERE ProjectGUID = @ProjectGuid
	   AND '*' In (@AreaParam) OR CONVERT(NVARCHAR, AreaSK) In (@AreaParam)
;

DECLARE @ChildAreas TABLE (AreaSK int, [AreaPath] nvarchar(4000) null);
INSERT @ChildAreas 
	SELECT DISTINCT a.AreaSK, a.[AreaPath]
	  FROM DimArea a
	 INNER JOIN @SelectedAreaList sa ON a.[AreaPath] LIKE sa.[AreaPath] + '\%';
;

DECLARE @AreasAndChildren TABLE (AreaSK int, [AreaPath]  nvarchar(4000) null);
INSERT @AreasAndChildren 

	SELECT AreaSK, [AreaPath] FROM @SelectedAreaList
	UNION
	SELECT AreaSK, [AreaPath] FROM @ChildAreas
;

-- No selected Area = all Areas
IF((SELECT COUNT(*) FROM @AreasAndChildren) = 0)
	INSERT @AreasAndChildren 
		SELECT AreaSK, [AreaPath] FROM DimArea
;

--
-- Get work items in the Areas of interest
--
DECLARE @WorkItemsInArea TABLE
(
	[System_State] nvarchar(256),
	[System_Id] int,
	[IterationPath] nvarchar(4000),
	[System_WorkItemType] nvarchar(256),
	[Microsoft_VSTS_Scheduling_RemainingWork] float null,
	[Microsoft_VSTS_Scheduling_CompletedWork] float null,
	[Microsoft_VSTS_Scheduling_OriginalEstimate] float null
);

INSERT @WorkItemsInArea
    SELECT wi.[System_State],
		wi.[System_Id],
		wi.[IterationPath],
		wi.[System_WorkItemType],
		wi.[Microsoft_VSTS_Scheduling_RemainingWork],
		wi.[Microsoft_VSTS_Scheduling_CompletedWork],
		wi.[Microsoft_VSTS_Scheduling_OriginalEstimate]
	FROM [CurrentWorkItemView] wi JOIN  @AreasAndChildren areas
		ON wi.[ProjectNodeGUID] = @ProjectGuid
		AND wi.[System_IsDeleted] = 0
		AND wi.[AreaPath] = areas.[AreaPath]
		AND wi.[System_State] &lt;&gt; @Proposed
;

--
-- Get the list of iterations, with levels in the hierarchy
--
DECLARE @Iterations TABLE 
(
	ID int PRIMARY KEY, 
	ParentID int, 
	Iteration nvarchar(256), 
	[Iteration Path] nvarchar(4000),
	IterationLevel int
);
WITH
IterationTree(IterationSK, ParentIterationSK, IterationName, IterationPath, IterationLevel) AS
(
	-- Anchor Member, starts with the top-level iteration
	SELECT	IterationSK , ParentIterationSK AS ParentID, IterationName, [IterationPath], 0
	  FROM	DimIteration
	 WHERE	[ProjectGUID] = @ProjectGuid
	   AND  [IterationName] &lt;&gt; 'Deleted Nodes'
	   AND	[IterationSK] = ParentIterationSK

	UNION ALL

	-- Recursive Member
	SELECT	i.IterationSK, i.ParentIterationSK, i.IterationName, i.[IterationPath], it.IterationLevel + 1
	  FROM	DimIteration i INNER JOIN IterationTree it ON i.ParentIterationSK = it.IterationSK
	 WHERE  i.IterationSK &lt;&gt; i.ParentIterationSK
)

INSERT @Iterations
	SELECT IterationSK AS ID, ParentIterationSK AS ParentID, Space(2 * IterationLevel) + IterationName AS Iteration, IterationPath AS [Iteration Path], IterationLevel
	FROM IterationTree ORDER BY [Iteration Path]
;

--
-- Raw counts
--
-- Separate Bugs into columns per state. This is so that we can assign specific colors.
-- count of bugs for every iteration, including zeros
DECLARE @Bugs TABLE
(
	[Iteration Path] nvarchar(4000) ,
	Proposed int,
	Active int,
	Resolved int,
	Closed int,
	AllBugs int
);
INSERT @Bugs
	SELECT  i.[Iteration Path],
		SUM(CASE wi.[System_State] WHEN @Proposed THEN 1 ELSE 0 END) as Proposed,
		SUM(CASE wi.[System_State] WHEN @Active THEN 1 ELSE 0 END) as Active,
		SUM(CASE wi.[System_State] WHEN @Resolved THEN 1 ELSE 0 END) as Resolved,
		SUM(CASE wi.[System_State] WHEN @Closed THEN 1 ELSE 0 END) as Closed,
		COUNT(wi.[System_Id]) as [AllBugs] 
	FROM @Iterations i LEFT OUTER JOIN @WorkItemsInArea wi 
		ON i.[Iteration Path] = wi.[IterationPath] AND (wi.[System_WorkItemType] = @Bug) 
	GROUP BY i.[Iteration Path]
; 

-- Each Iteration has two Work rows - one in which Remaining really is the Remaining Work, and one in which it's the Original Estimate.
-- This enables the chart to display two bars per Iteration.
-- IsOriginal flags which value the Remaining field represents. It must be numeric, to enable control of the bar widths in the chart.
-- All work items in all states are included. (If users don't want to include Closed items, they should set their remaining work to 0.)
DECLARE @Work TABLE
(
	[Iteration Path] nvarchar(4000) ,
	Remaining float,
	Completed float,
	Original float
);
INSERT @Work
	SELECT wi.[IterationPath] AS [Iteration Path], 
		SUM(ISNULL(wi.[Microsoft_VSTS_Scheduling_RemainingWork], 0)) AS Remaining, 
		SUM(ISNULL(wi.[Microsoft_VSTS_Scheduling_CompletedWork], 0)) AS Completed,
		SUM(ISNULL(wi.[Microsoft_VSTS_Scheduling_OriginalEstimate], 0)) AS Original
	FROM  @WorkItemsInArea wi
	GROUP BY wi.[IterationPath]
;

-- Count Stories/Deliverables that have no child stories
DECLARE @StoryCounts TABLE
(
	[Iteration Path] nvarchar(4000) ,
	OpenStories int,
	ClosedStories int
);
INSERT @StoryCounts
	SELECT  wi.[IterationPath] AS [Iteration Path], 
		SUM(CASE WHEN wi.[System_State] &lt;&gt; @Closed THEN 1 ELSE 0 END) OpenStories,
		SUM(CASE WHEN wi.[System_State] = @Closed THEN 1 ELSE 0 END) ClosedStories
	FROM @WorkItemsInArea wi 
		LEFT OUTER JOIN FactWorkItemLinkHistory lh
		ON lh.SourceWorkItemID = wi.[System_Id] 
			AND lh.WorkItemLinkTypeSK = @ChildWorkItemLinkTypeSK
			AND lh.RemovedDate = CONVERT(DATETIME, '9999', 126)
			AND lh.TeamProjectCollectionSK = @TeamProjectCollectionSK
		LEFT OUTER JOIN [CurrentWorkItemView] child 
		ON lh.TargetWorkItemID = child.[System_Id] 
			AND child.[System_WorkItemType] IN (@DeliverableCategory)
			AND child.ProjectNodeGUID = @ProjectGuid
			AND child.[System_IsDeleted] = 0
	WHERE wi.[System_WorkItemType] IN (@DeliverableCategory)
		AND child.[System_Id] IS NULL -- exclude stories with child stories
	GROUP BY  wi.[IterationPath]
;


DECLARE @TestResults TABLE
(
	[Iteration Path] nvarchar(4000) ,
	Passed int,
	Failed int,
	NotRun int
);

/* TEMPORARILY REMOVING REFERENCES TO TEST RESULTS
