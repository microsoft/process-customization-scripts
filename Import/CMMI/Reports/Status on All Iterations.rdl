DECLARE @SumOfChildren TABLE 
([Iteration Path] nvarchar(4000)
	, OpenStories int null, ClosedStories int null
	, Remaining float null, Completed float null, Original float null
	, Proposed int null, Active int null, Resolved int null, Closed int null, AllBugs int null
	, Passed int null, Failed int null, NotRun int null);


INSERT @SumOfChildren -- Summed values for the children of each iteration.
	SELECT i.[Iteration Path]
		, SUM(OpenStories) OpenStories, SUM (ClosedStories) ClosedStories
		, SUM(Remaining) Remaining, SUM(Completed) Completed, SUM(Original) Original
		, SUM(Proposed) Proposed, SUM(Active) Active, SUM(Resolved) Resolved, SUM(Closed) Closed, SUM(AllBugs) AllBugs	
		, SUM(Passed) Passed, SUM(Failed) Failed, SUM(NotRun) NotRun
	FROM @Iterations i 
		LEFT OUTER JOIN @Iterations child ON child.[Iteration Path] LIKE i.[Iteration Path] + '\%'
		LEFT OUTER JOIN @StoryCounts stories ON stories.[Iteration Path] = child.[Iteration Path]
		LEFT OUTER JOIN @Bugs bugs ON bugs.[Iteration Path] = child.[Iteration Path] 
		LEFT OUTER JOIN @Work work ON work.[Iteration Path] = child.[Iteration Path]
		LEFT OUTER JOIN @TestResults results ON results.[Iteration Path] = child.[Iteration Path]
	GROUP BY i.[Iteration Path]
;

DECLARE @InclusiveSums TABLE
(	ID int, ParentID int null, Iteration nvarchar(256), [Iteration Path] nvarchar(4000), IterationLevel int
	, RawProposed int null, RawActive int null, RawResolved int null, RawClosed int null, RawAllBugs int null
	, Proposed int null, Active int null, Resolved int null, Closed int null, AllBugs int null
	, RawRemaining float null, RawCompleted float null, RawOriginal float null
	, Remaining float null, Completed float null, Original float null
	, RawOpenStories int null, RawClosedStories int null
	, OpenStories int null, ClosedStories int null
	, RawPassed int null, RawFailed int null, RawNotRun int null
	, Passed int null, Failed int null, NotRun int null
);
INSERT @InclusiveSums  -- Sum including the current total. Each iteration shows its own + children's values.
   SELECT i.ID, i.ParentID, i.Iteration, i.[Iteration Path], i.IterationLevel, 
		bugs.Proposed as RawProposed, bugs.Active as RawActive, bugs.Resolved as RawResolved, bugs.Closed as RawClosed, bugs.AllBugs as RawAllBugs, 
			ISNULL(bugs.Proposed, 0) + ISNULL(sumOfChildren.Proposed, 0) as Proposed,
			ISNULL(bugs.Active, 0) + ISNULL(sumOfChildren.Active, 0) as Active,
			ISNULL(bugs.Resolved, 0) + ISNULL(sumOfChildren.Resolved, 0) as Resolved,
			ISNULL(bugs.Closed, 0) + ISNULL(sumOfChildren.Closed, 0) as Closed,
			ISNULL(bugs.AllBugs, 0) + ISNULL(sumOfChildren.AllBugs, 0) as AllBugs, 
		work.Remaining as RawRemaining, work.Completed as RawCompleted, work.Original as RawOriginal,
			ISNULL(work.Remaining, 0) + ISNULL(sumOfChildren.Remaining, 0) as Remaining,
			ISNULL(work.Completed, 0) + ISNULL(sumOfChildren.Completed, 0) as Completed,
			ISNULL(work.Original, 0) + ISNULL(sumOfChildren.Original, 0) as Original,
		stories.OpenStories as RawOpenStories, stories.ClosedStories as RawClosedStories,
			ISNULL(stories.OpenStories, 0) + ISNULL(sumOfChildren.OpenStories, 0) as OpenStories,
			ISNULL(stories.ClosedStories, 0) + ISNULL(sumOfChildren.ClosedStories, 0) as ClosedStories,
		results.Passed as RawPassed, results.Failed as RawFailed,results.NotRun as RawNotRun,
			ISNULL(results.Passed, 0) + ISNULL(sumOfChildren.Passed, 0) as Passed,
			ISNULL(results.Failed, 0) + ISNULL(sumOfChildren.Failed, 0) as Failed,
			ISNULL(results.NotRun, 0) + ISNULL(sumOfChildren.NotRun, 0) as NotRun
   FROM @Iterations i
	LEFT OUTER JOIN @Work work ON (work.[Iteration Path] = i.[Iteration Path])
	LEFT OUTER JOIN @StoryCounts stories ON (stories.[Iteration Path] = i.[Iteration Path])
	LEFT OUTER JOIN @Bugs bugs ON (bugs.[Iteration Path] = i.[Iteration Path])
	LEFT OUTER JOIN @TestResults results ON (results.[Iteration Path] = i.[Iteration Path])
	LEFT OUTER JOIN @SumOfChildren sumOfChildren ON sumOfChildren.[Iteration Path] = i.[Iteration Path]
;


IF (@DisplayOwnValues &gt; 0)
BEGIN
INSERT @InclusiveSums  -- Add extra rows for parents that have their own values as well as values rolled up from children.
	-- Keep the parent correct for Max calculations; create new ID so as to avoid looking like a root. New Iteration name is ".". Substitute Raw values into reported fields.
	SELECT -ID as ID, ID as ParentID, Iteration + ' '  + @OwnValuesTag AS Iteration, [Iteration Path] + '\'+@OwnValuesTag AS [Iteration Path], IterationLevel + 1 as IterationLevel, 
		RawProposed, RawActive, RawResolved, RawClosed, RawAllBugs, 
		ISNULL(RawProposed, 0) AS Proposed, ISNULL(RawActive, 0) AS Active, ISNULL(RawResolved, 0) AS Resolved, ISNULL(RawClosed, 0) AS Closed, ISNULL(RawAllBugs, 0) AS AllBugs,
		RawRemaining, RawCompleted, RawOriginal, 
		ISNULL(RawRemaining, 0) AS Remaining, ISNULL(RawCompleted, 0) AS Completed, ISNULL(RawOriginal, 0) AS Original,
		RawOpenStories, RawClosedStories,
		ISNULL(RawOpenStories, 0) as OpenStories, ISNULL(RawClosedStories, 0) as ClosedStories,
		RawPassed, RawFailed, RawNotRun,
		ISNULL(RawPassed, 0) AS Passed, ISNULL(RawFailed, 0) AS Failed, ISNULL(RawNotRun, 0) AS NotRun
	FROM @InclusiveSums
	WHERE AllBugs &gt; RawAllBugs AND RawAllBugs &gt; 0 OR Remaining &gt; RawRemaining AND RawRemaining &gt; 0 OR Completed &gt; RawCompleted AND RawCompleted &gt; 0 
			OR Original &gt; RawOriginal AND RawOriginal &gt; 0 OR Passed &gt; RawPassed AND RawPassed &gt; 0 OR NotRun &gt; RawNotRun AND RawNotRun &gt; 0

END

--
-- Max values are used to scale the bars in the charts.
-- Siblings share a max value. 
-- (We don't have the same max across all the iterations because this makes children and grandchildren look very small.)
--
DECLARE @MaxOfChildren TABLE (ID int, MaxAllBugs int null, MaxStories int null, MaxWork float null, MaxOriginal float null, MaxTestResults int null);

INSERT @MaxOfChildren  -- Max values for the children of each iteration, assigned to the parent.
    SELECT i.ID, MAX(AllBugs) MaxAllBugs, MAX(OpenStories + ClosedStories) MaxStories, MAX(Remaining+Completed) MaxWork, 
		MAX(Original) MaxOriginal, MAX(Passed+Failed+NotRun) MaxTestResults
	FROM @Iterations i LEFT OUTER JOIN @InclusiveSums s ON (s.[Iteration Path] LIKE i.[Iteration Path] + '\%')
	GROUP BY i.ID
;
DECLARE @RootMax TABLE (MaxAllBugs int null, MaxStories int null, MaxWork float null, MaxOriginal float null, MaxTestResults int null);
INSERT @RootMax  -- The max value for the top iterations
	SELECT MAX(AllBugs) as MaxAllBugs, MAX(OpenStories+ClosedStories) MaxStories, MAX(Remaining+Completed) MaxWork, MAX(Original) MaxOriginal, MAX(Passed + Failed + NotRun) MaxTestResults
	FROM @InclusiveSums WHERE ID = ParentID
;
DECLARE @RootMaxWork float;
SET @RootMaxWork = ( SELECT CASE WHEN MaxWork &gt; MaxOriginal THEN MaxWork ELSE MaxOriginal END FROM @RootMax )
;
DECLARE @ParentMax TABLE (ID int, ParentID int null, Iteration nvarchar(256), IterationLevel int, [Iteration Path] nvarchar(4000), IsOriginal int,
	RawProposed int null, RawActive int null, RawResolved int null, RawClosed int null, RawAllBugs int null, 
	Proposed int null, Active int null, Resolved int null, Closed int null, AllBugs int null, MaxAllBugs int null,
	RawRemaining float null, RawCompleted float null, RawOriginal float null, Remaining float null, Completed float null, Original float null, MaxWork float null,
	RawOpenStories int null, RawClosedStories int null, OpenStories int null, ClosedStories int null, MaxStories int null,
	RawPassed int null, RawFailed int null, RawNotRun int null, Passed int null, Failed int null, NotRun int null, MaxTestResults int null
);


INSERT @ParentMax -- Assign the max values to children.
	-- Root iterations have ID==ParentID. Also combine MaxWork and MaxOriginal, which will be on same scale.
	SELECT i.ID, i.ParentID, Iteration, i.IterationLevel, i.[Iteration Path], 0 AS IsOriginal,
		RawProposed, RawActive, RawResolved, RawClosed, RawAllBugs, Proposed, Active, Resolved, Closed, AllBugs,
			CASE WHEN i.ID = i.ParentID THEN rootMax.MaxAllBugs ELSE maxOfChildren.MaxAllBugs END MaxAllBugs,
		RawRemaining, RawCompleted, RawOriginal, Remaining, Completed, Original,
			CASE WHEN i.ID = i.ParentID THEN @RootMaxWork 
			ELSE CASE WHEN maxOfChildren.MaxOriginal &gt; maxOfChildren.MaxWork THEN maxOfChildren.MaxOriginal ELSE maxOfChildren.MaxWork END 
			END MaxWork,
		RawOpenStories, RawClosedStories, OpenStories, ClosedStories, 
			CASE WHEN i.ID = i.ParentID THEN RootMax.MaxStories ELSE maxOfChildren.MaxStories END MaxStories,
		RawPassed, RawFailed, RawNotRun, Passed, Failed, NotRun,
			CASE WHEN i.ID = i.ParentID THEN RootMax.MaxTestResults ELSE maxOfChildren.MaxTestResults END MaxTestResults
	FROM @InclusiveSums i JOIN @MaxOfChildren maxOfChildren ON i.ParentID = maxOfChildren.ID CROSS JOIN @RootMax rootMax
;


-- Each Iteration can have two Work rows - one in which Remaining really is the Remaining Work, and one in which it's the Original Estimate.
-- This enables the chart to display two bars per Iteration.
-- IsOriginal flags which value the Remaining field represents. It must be numeric, to enable control of the bar widths in the chart.

INSERT @ParentMax
	SELECT ID, ParentID, Iteration, IterationLevel,
		[Iteration Path], 
		1 AS IsOriginal, -- Flag as a row where the Original value is substituted into Remaining
		RawProposed, RawActive, RawResolved, RawClosed, RawAllBugs, Proposed, Active, Resolved, Closed, AllBugs, 0 AS MaxAllBugs,
		RawRemaining, RawCompleted, RawOriginal, 
		Original AS Remaining, 0 AS Completed, -- Substitutions for IsOriginal
		Original, MaxWork,
		RawOpenStories, RawClosedStories, OpenStories, ClosedStories, 0 as MaxStories,
		RawPassed, RawFailed, RawNotRun, Passed, Failed, NotRun, 0 AS MaxTestResults
	FROM @ParentMax
	WHERE Original &gt; 0 

-- Count project-level iterations
DECLARE @RootCount int;
SET @RootCount = (SELECT COUNT(*) FROM @ParentMax WHERE IterationLevel=0 AND IsOriginal = 0);

SELECT ID, ParentID, Iteration, 
   CASE WHEN @RootCount &gt; 1 THEN  IterationLevel ELSE IterationLevel - 1 END AS IterationLevel,
   [Iteration Path], IsOriginal,
   RawProposed, RawActive, RawResolved, RawClosed, RawAllBugs, Proposed, Active, Resolved, Closed, AllBugs,MaxAllBugs,
   RawRemaining, RawCompleted, RawOriginal, Remaining, Completed, Original, MaxWork, @RootMaxWork as RootMaxWork,
		RawOpenStories, RawClosedStories, OpenStories, ClosedStories, MaxStories,
   RawPassed, RawFailed, RawNotRun, Passed, Failed, NotRun, MaxTestResults
FROM @ParentMax 
WHERE IterationLevel &gt; 0  OR @RootCount &gt; 1 -- Show top level only if there are more than one
ORDER BY [Iteration Path];</CommandText>
