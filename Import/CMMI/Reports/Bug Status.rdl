          <FilterExpression>=Fields!ParameterLevel.Value</FilterExpression>
          <Operator>NotEqual</Operator>
          <FilterValues>
            <FilterValue DataType="Integer">2</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
