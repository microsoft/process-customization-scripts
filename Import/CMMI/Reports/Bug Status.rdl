          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>TfsReportDS</DataSourceName>
        <CommandText>;WITH IterationKey(ProjectGUID, IterationSK, IterationName, Depth, StartDate, FinishDate, IterationKey) AS
(
	SELECT ProjectGUID, IterationSK, IterationName, Depth, StartDate, FinishDate,
		'.&amp;[' +
			CONVERT(nvarchar, CONVERT(bigint, SUBSTRING(CONVERT(binary(16), CONVERT(uniqueidentifier, IterationGUID)), 1, 8))) +
			']&amp;[' +
			CONVERT(nvarchar, CONVERT(bigint, SUBSTRING(CONVERT(binary(16), CONVERT(uniqueidentifier, IterationGUID)), 9, 8))) +
		']'
	  FROM DimIteration
)

SELECT '' AS ProjectGUID, '*' AS IterationSK, 'All' AS IterationName, NULL AS StartDate, NULL AS FinishDate
WHERE '[Work Item].[Iteration Hierarchy].[All]' IN (@IterationParam)

UNION

SELECT DISTINCT 
	CASE WHEN Depth = 0 THEN '' ELSE ProjectGUID END,
	CASE WHEN Depth = 0 THEN '*' ELSE CAST(IterationSK AS NVARCHAR(255)) END AS IterationSK, 
	CASE WHEN Depth = 0 THEN 'All' ELSE IterationName END,
	CASE WHEN Depth = 0 THEN NULL ELSE StartDate END,
	CASE WHEN Depth = 0 THEN NULL ELSE FinishDate END
  FROM IterationKey
 WHERE
	'[Work Item].[Iteration Hierarchy].[Iteration' + CONVERT(varchar, Depth) + ']' + IterationKey IN (@IterationParam)
    OR	
	'[Work Item].[Iteration Hierarchy].[Iteration' + CONVERT(varchar, Depth + 1) + ']' + IterationKey IN (@IterationParam)</CommandText>
        <QueryParameters>
