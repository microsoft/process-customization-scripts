  <Code>'// stock colors - keep (but retired)
'    Public Const RED As String = "#FF0042"
'    Public Const WHITE As String = "White"
'    Public Const LIGHTGREEN As String = "#D3FA83"
'    Public Const DARKGREEN As String = "#5B8007"
'    Public Const AMBER As String = "#FFBE00"
'    Public Const YELLOW As String = "#FFED36"

'// new color set
Public Const RED As String = "#c90012"
Public Const LIGHTRED As String = "#FF7586"
Public Const WHITE As String = "#ffffff"
Public Const LIGHTGREEN As String = "#d3fa83"
Public Const DARKGREEN As String = "#7bb876"
Public Const AMBER As String = "#f9934a"
Public Const YELLOW As String = "#ffed36"
Public Const PartiallySucceeded As Integer = 4

Public Function GetBlockColor(ByVal BuildFailCount As Integer, ByVal BuildSuccessCount As Integer, _
                              ByVal TestFailedCount As Integer, ByVal TestPassedCount As Integer, _
                              ByVal CodeCoverage As Double, _
                              ByVal CodeCoverageThreshold As Double, _
                              ByVal BuildStatus As Integer) As String

    '// This function returns a color value to describe a build state, it is used in the cells of the chart

    Dim retval As String

    If BuildStatus = PartiallySucceeded Then
        ' partially succeded build
        retval = LIGHTRED

    ElseIf BuildFailCount = 0 And BuildSuccessCount = 0 Then
        ' no builds
        retval = WHITE

    ElseIf BuildFailCount &gt; 0 Then
        ' build failed
        retval = RED

    ElseIf TestFailedCount &gt; 0 Then
        ' tests failed
        retval = AMBER

    ElseIf TestPassedCount &gt; 0 And CodeCoverage &lt; CodeCoverageThreshold Then
        ' tests passed, but low coverage
        retval = LIGHTGREEN

    ElseIf TestPassedCount &gt; 0 Then
        ' tests passed
        retval = DARKGREEN

    Else
        ' build ran successfully but no tests
        retval = YELLOW

    End If

    Return retval

End Function


Public Function GetBlockLabel(ByVal BuildFailCount As Integer, ByVal BuildSuccessCount As Integer, _
                              ByVal TestFailedCount As Integer, ByVal TestPassedCount As Integer, _
                              ByVal CodeCoverage As Double, _
                              ByVal CodeCoverageThreshold As Double) As String

    '// This function returns a string to describe a build state, it is used in the XML output

    Dim retval As String

    ' no builds
    If BuildFailCount = 0 And BuildSuccessCount = 0 Then
        retval = "NoBuilds"

        ' build failed
    ElseIf BuildFailCount &gt; 0 Then
        retval = "BuildFailed"

        ' tests failed
    ElseIf TestFailedCount &gt; 0 Then
        retval = "TestsFailed"

        ' tests passed, but low coverage
    ElseIf TestPassedCount &gt; 0 And CodeCoverage &lt; CodeCoverageThreshold Then
        retval = "LowCoverage"

        ' tests passed
    ElseIf TestPassedCount &gt; 0 Then
        retval = "TestsPassed"

        ' build ran successfully but no tests
    Else
        retval = "NoTests"

    End If

    Return retval

End Function

Public Function GetShortDayMonthString() As String
    Dim info As System.Globalization.DateTimeFormatInfo = System.Globalization.CultureInfo.CurrentCulture.DateTimeFormat
    Return info.MonthDayPattern.Replace("MMMM", "MMM")
End Function

Function SqlToMdx(ByVal values As Object(), ByVal pattern As String) As String()
    Dim results(values.Length - 1) As String
    For i As Integer = 0 To values.Length - 1
        If values(i).ToString = "*" Then
            results(i) = String.Format(pattern, "All")
        Else
            results(i) = String.Format(pattern, values(i))
        End If
    Next

    Return results
End Function

