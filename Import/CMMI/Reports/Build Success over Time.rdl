    </DataSet>
    <DataSet Name="PlatformParam">
      <Fields>
        <Field Name="ParameterValue">
          <DataField>ParameterValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ParameterCaption">
          <DataField>ParameterCaption</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>TfsReportDS</DataSourceName>
        <CommandText>
          SELECT DISTINCT BuildPlatformName as ParameterValue, BuildPlatformName AS ParameterCaption
          FROM	BuildProjectView
          WHERE	TeamProjectProjectNodeGUID = @ProjectGuid

          UNION

          SELECT	'*' AS Expr1, @AllNoFilter AS Expr2

          ORDER BY BuildPlatformName
        </CommandText>
        <QueryParameters>
