<?xml version="1.0" encoding="utf-8"?>
<WorkItemQuery Version="1">
  <Wiql>
		SELECT [System.Id], [System.State], [Microsoft.VSTS.Common.Priority], [System.AssignedTo], [System.Title] 
		FROM WorkItems WHERE [System.TeamProject] = @project  AND  [System.WorkItemType] = 'Task'  
		AND  	[Microsoft.VSTS.CMMI.TaskType] = 'Corrective Action' 
		ORDER BY [System.Id]
	</Wiql>
</WorkItemQuery>