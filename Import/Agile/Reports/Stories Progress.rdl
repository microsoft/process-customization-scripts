        </Field>
        <Field Name="RootMax">
          <DataField>RootMax</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Remaining">
          <DataField>Remaining</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Completed">
          <DataField>Completed</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
