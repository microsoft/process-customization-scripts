  <Code>public const DARKGREEN as String = "#805b8007"

	Function MdxToSql(ByVal value As String) As String
		If value.EndsWith("[All]") Then
			Return "*"
		Else
			Dim index As Integer = value.LastIndexOf("[")
			Dim newValue As String = value.Substring(index + 1)
			Return newValue.Substring(0, newValue.Length - 1)
		End If
	End Function

	Function ParametersToString(ByVal values As Object()) As String
		Dim str As System.Text.StringBuilder = New System.Text.StringBuilder()

		For i As Integer = 0 To values.Length - 1
			If i &gt; 0 Then str.Append(", ")
			str.Append(Trim(values(i).ToString))
		Next
		Return str.ToString
	End Function
</Code>
