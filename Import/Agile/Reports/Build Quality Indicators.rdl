    <ReportItems>
      <Chart Name="dundasChartControl1">
        <ChartCategoryHierarchy>
          <ChartMembers>
            <ChartMember>
              <Group Name="BuildGroup">
                <GroupExpressions>
                  <GroupExpression>=Fields!Build.Value</GroupExpression>
                </GroupExpressions>
              </Group>
              <Label>=IIF(Fields!Build.Value Is Nothing, " ", Fields!Build.Value)</Label>
            </ChartMember>
          </ChartMembers>
        </ChartCategoryHierarchy>
        <ChartSeriesHierarchy>
          <ChartMembers>
            <ChartMember>
              <Label>Passed Tests</Label>
            </ChartMember>
            <ChartMember>
              <Label>Failed Tests</Label>
            </ChartMember>
            <ChartMember>
              <Label>Inconclusive Tests</Label>
            </ChartMember>
            <ChartMember>
              <Label>Active Bugs (count)</Label>
            </ChartMember>
            <ChartMember>
              <Label>Code Churn (lines)</Label>
            </ChartMember>
            <ChartMember>
              <Label>Code Coverage (percent)</Label>
            </ChartMember>
          </ChartMembers>
        </ChartSeriesHierarchy>
        <ChartData>
          <ChartSeriesCollection>
            <ChartSeries Name="Series1">
              <ChartDataPoints>
                <ChartDataPoint>
                  <ChartDataPointValues>
                    <Y>=Fields!Passing_Result.Value / Max(CINT(Fields!Total_Results.Value), "dsChartData") * 100</Y>
                  </ChartDataPointValues>
                  <ChartDataLabel>
                    <Style>
                      <FontFamily>tahoma</FontFamily>
                      <FontSize>8pt</FontSize>
                    </Style>
                    <Label>=Iif(Fields!Passing_Result.Value &gt; 0, Fields!Passing_Result.Value, "")</Label>
                    <Visible>true</Visible>
                  </ChartDataLabel>
                  <Style>
                    <Border>
                      <Color>#5b8007</Color>
                      <Style>None</Style>
                    </Border>
                    <Color>#609bdb00</Color>
                    <BackgroundGradientEndColor>#aadc43</BackgroundGradientEndColor>
                  </Style>
                  <ChartMarker>
                    <Style />
                  </ChartMarker>
                  <CustomProperties>
                    <CustomProperty>
                      <Name>LabelStyle</Name>
                      <Value>Top</Value>
                    </CustomProperty>
                  </CustomProperties>
                </ChartDataPoint>
              </ChartDataPoints>
              <Subtype>Stacked</Subtype>
              <Style>
                <ShadowOffset>0pt</ShadowOffset>
              </Style>
              <ChartEmptyPoints>
                <Style />
                <ChartMarker>
                  <Style />
                </ChartMarker>
                <ChartDataLabel>
                  <Style />
                </ChartDataLabel>
              </ChartEmptyPoints>
              <LegendName>Legend1</LegendName>
              <ChartAreaName>Default</ChartAreaName>
              <ValueAxisName>Primary</ValueAxisName>
              <CategoryAxisName>Primary</CategoryAxisName>
              <ChartSmartLabel>
                <Disabled>true</Disabled>
                <MaxMovingDistance>22.5pt</MaxMovingDistance>
              </ChartSmartLabel>
            </ChartSeries>
            <ChartSeries Name="Series2">
              <ChartDataPoints>
                <ChartDataPoint>
                  <ChartDataPointValues>
                    <Y>=Fields!Failing_Result.Value / Max(CINT(Fields!Total_Results.Value), "dsChartData") * 100</Y>
                  </ChartDataPointValues>
                  <ChartDataLabel>
