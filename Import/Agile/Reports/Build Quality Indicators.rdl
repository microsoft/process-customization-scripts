          <DataField>&lt;?xml version="1.0" encoding="utf-8"?&gt;&lt;Field xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:type="Measure" UniqueName="[Measures].[Total Results]" /&gt;</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Passing_Result">
          <DataField>&lt;?xml version="1.0" encoding="utf-8"?&gt;&lt;Field xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:type="Measure" UniqueName="[Measures].[Passing Result]" /&gt;</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Failing_Result">
          <DataField>&lt;?xml version="1.0" encoding="utf-8"?&gt;&lt;Field xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:type="Measure" UniqueName="[Measures].[Failing Result]" /&gt;</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Inconclusive_Result">
          <DataField>&lt;?xml version="1.0" encoding="utf-8"?&gt;&lt;Field xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:type="Measure" UniqueName="[Measures].[Inconclusive Result]" /&gt;</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>TfsOlapReportDS</DataSourceName>
        <CommandText>WITH
MEMBER [Measures].[Build Start Time] AS
	[Build].[Build Start Time].CurrentMember.MemberValue

MEMBER [Measures].[Build Date] AS
	"[Date].[Date].&amp;[" + Format(DatePart("yyyy", [Measures].[Build Start Time]), "0000") + "-" +
	Format(DatePart("m", [Measures].[Build Start Time]), "00") + "-" +
	Format(DatePart("d", [Measures].[Build Start Time]), "00")+ "T00:00:00]"

MEMBER [Measures].[Passing Result] AS
	([Test Result].[Outcome Id].&amp;[2], [Measures].[Result Count Trend]) + 0

MEMBER [Measures].[Failing Result] AS
	([Test Result].[Outcome Id].&amp;[3], [Measures].[Result Count Trend]) + 0

MEMBER [Measures].[Inconclusive Result] AS
	([Test Result].[Outcome Id].&amp;[4], [Measures].[Result Count Trend]) + 0

MEMBER [Measures].[Total Results] AS
	0 + [Measures].[Passing Result] + [Measures].[Failing Result] + [Measures].[Inconclusive Result]

MEMBER [Measures].[Active Bugs] AS
	(
		[Measures].[Work Item Count],
		 STRTOMEMBER("[Work Item].[System_State].[" + @Active + "]"),
		 STRTOMEMBER("[Work Item].[System_WorkItemType].[" + @Bug + "]"),
		 STRTOMEMBER("[Work Item].[System_IsDeleted].&amp;[false]"),
		 STRTOMEMBER([Measures].[Build Date])
	) + 0

MEMBER [Measures].[Percent Blocks] AS
	([Measures].[Blocks Covered] / ([Measures].[Blocks Covered] + [Measures].[Blocks Not Covered] )) + 0

MEMBER [Measures].[Code Churn] AS
	[Measures].[Total Churn] + 0

SELECT
{
	[Measures].[Active Bugs],
	[Measures].[Code Churn],
	[Measures].[Percent Blocks],
	[Measures].[Total Results],
	[Measures].[Passing Result],
	[Measures].[Failing Result],
	[Measures].[Inconclusive Result]
} ON COLUMNS,
{
	Tail(
		Filter(
			NonEmpty(
				[Build].[Build].[All].Children,
				[Measures].[Build Project Count]
			),
			[Measures].[Build Start Time] &lt;= CDate(@EndDateParam) + 1
		),
		@DefaultNumberOfBuildsToSelect
	)
} ON ROWS
FROM
(
	SELECT
	(
		StrToMember("[Team Project].[Project Node GUID].&amp;[{" + @ProjectGuid + "}]"),
		// Filter or System_IsDeleted field is included in [Active Bugs] measure.
		StrToSet(@AreaParam),
		//
		// We've designed this report so you have to select a single value for Platform, Configuration, and
		// Build Definition. These parameters default to the [All] value, which we've mapped to the string
		// parameter @ChooseValue. Here we ignore these values and only return values once you've selected
		// values for all three parameters.
		//
		Except(
			StrToSet(@PlatformParam),
			[Build Platform].[Build Platform].[All]
		),
		Except(
			StrToSet(@FlavorParam),
			[Build Flavor].[Build Flavor].[All]
		),
		Except(
			StrToSet(@BuildTypeParam),
			[Build].[Build Definition Name].[All]
		)
	) ON 0
	FROM [Team System]
)</CommandText>
        <QueryParameters>
          <QueryParameter Name="ProjectGuid">
            <Value>=Parameters!ProjectGuid.Value</Value>
          </QueryParameter>
          <QueryParameter Name="BuildTypeParam">
            <Value>=Parameters!BuildTypeParam.Value</Value>
          </QueryParameter>
          <QueryParameter Name="PlatformParam">
            <Value>=Parameters!PlatformParam.Value</Value>
          </QueryParameter>
          <QueryParameter Name="FlavorParam">
            <Value>=Parameters!FlavorParam.Value</Value>
          </QueryParameter>
          <QueryParameter Name="Active">
            <Value>=Parameters!Active.Value</Value>
          </QueryParameter>
          <QueryParameter Name="Bug">
            <Value>=Parameters!Bug.Value</Value>
          </QueryParameter>
          <QueryParameter Name="AreaParam">
            <Value>=Parameters!AreaParam.Value</Value>
          </QueryParameter>
          <QueryParameter Name="EndDateParam">
            <Value>=Parameters!EndDateParam.Value</Value>
          </QueryParameter>
          <QueryParameter Name="DefaultNumberOfBuildsToSelect">
            <Value>=Parameters!DefaultNumberOfBuildsToSelect.Value</Value>
          </QueryParameter>
        </QueryParameters>
